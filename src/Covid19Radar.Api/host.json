{
    "version": "2.0",
    "extensions": {
        "http": {
            "maxOutstandingRequests": 400,
            "maxConcurrentRequests": 25
        }
    },
    "logging": {
        "logLevel": {
            "default": "Information"
        },
        "applicationInsights": {
            "samplingExcludedTypes": "Request;Exception",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    }
}